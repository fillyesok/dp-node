{
  "name": "dp-node",
  "version": "0.1.14",
  "description": "Faster, cheaper, better web framework for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "testonly": "mocha",
    "check": "node ./node_modules/npm-check/bin/cli.js",
    "lint": "standard",
    "lintfix": "standard --fix",
    "lintmon": "./node_modules/nodemon/bin/nodemon.js --exec './node_modules/standard/bin/cmd.js'"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/why2pac/dp-node.git"
  },
  "keywords": [
    "MVC",
    "Web",
    "Framework",
    "Express"
  ],
  "author": "GONZO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/why2pac/dp-node/issues"
  },
  "homepage": "https://github.com/why2pac/dp-node#readme",
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/lib/static/**/*.js"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "camelcase": "^5.0.0",
    "compression": "^1.7.2",
    "cookie": "0.3.1",
    "cookie-parser": "^1.4.3",
    "cookie-signature": "^1.1.0",
    "decamelize": "^2.0.0",
    "express": "^4.16.3",
    "express-naked-redirect": "^0.1.3",
    "fs-extra": "^6.0.1",
    "handlebars": "^4.0.11",
    "handlebars-helper-repeat": "^2.0.0",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^3.12.1",
    "html-minifier": "^3.5.18",
    "http-errors": "^1.6.3",
    "ini": "^1.3.5",
    "knex": "^0.15.0",
    "morgan": "^1.9.0",
    "promised-handlebars": "^2.0.1",
    "redis": "^2.8.0",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "command-line-test": "^1.0.10",
    "find-port-sync": "^1.1.1",
    "mocha": "^5.2.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.5",
    "npm-check": "^5.7.1",
    "should": "^13.2.1",
    "standard": "^11.0.1",
    "supertest": "^3.1.0"
  }
}
